{
  "name": "leroy-merlin-br/laravel-optimizely",
  "description": "A Laravel wrapper for the Optimizely SDK",
  "type": "library",
  "autoload": {
    "psr-4": {
      "LeroyMerlin\\Optimizely\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "require": {
    "php": ">7.1.0",
    "ext-json": "*",
    "illuminate/config": "^5.7.0 || ^6.0",
    "illuminate/support": "^5.7.0 || ^6.0",
    "optimizely/optimizely-sdk": "^3.2",
    "ext-redis": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.3",
    "mockery/mockery": "^1.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandre Ferreira Nogueira (@aleferrieranogueira)",
      "email": "ale.ferreiranogueira@gmail.com"
    }
